    {
      "cell_type": "code",
      "source": [
        "def cal_dis(speed, time):\n",
        "    print(\" time(km) :\", time)\n",
        "    print(\" Time(hr) :\", time)\n",
        "    return speed * time\n",
        "print(\" The calculated distance(km) :\",cal_dis(abs(42.6), abs(6.8)))\n",
        "print(\"\")\n",
        "\n"
