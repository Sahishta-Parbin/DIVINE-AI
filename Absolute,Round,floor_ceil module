        "def cal_speed(dist, time):\n",
        "    print(\" Distance(km) :\", dist)\n",
        "    print(\" Time(hr) :\", time)\n",
        "    return dist / time\n",
        "print(\" The calculated speed(km) :\",cal_speed(abs(62.9), abs(2.5)))\n",
        "print(\"\")\n",
        "\n"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "afWU08F7iQrH",
